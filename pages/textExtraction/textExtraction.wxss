/* pages/textExtraction/textExtraction.wxml.wxss */
.textExtraction{
  width: 100%;
  padding: 40rpx;
  box-sizing: border-box;
}
.textExtraction .upload{
  width: 100%;
  height: 400rpx;
  position: relative;

}
.textExtraction image{
  width: 100%;
  height: 400rpx;
  border-radius: 24rpx;
}

.textExtraction .photo{
  width: 180rpx;
  height: 180rpx;
  position: absolute;
  top: 100rpx;
  left: 36%;
  background:#e98d3f;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;

}
.textExtraction .photo image{
  width: 100rpx;
  height: 80rpx;
}

.textExtraction .upBtn{
  width: 260rpx;
  font-size: 32rpx;
  padding: 0;
  margin-top: 60rpx;
  background: #e98d3f;
  color: #fff;
}